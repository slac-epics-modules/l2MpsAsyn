### Threshold information ###
file "RegRO.template" { pattern
{ R,                DESC,                                       PARAM,                  ADDR       }
{ C0I0_THR_CH,      "Bay $(BAY) Ch 0 Int 0 Threshold channel",  BLM_THRNUM_$(BAY)00,    "$(BAY)"   }
{ C1I0_THR_CH,      "Bay $(BAY) Ch 1 Int 0 Threshold channel",  BLM_THRNUM_$(BAY)10,    "$(BAY)"   }
{ C2I0_THR_CH,      "Bay $(BAY) Ch 2 Int 0 Threshold channel",  BLM_THRNUM_$(BAY)20,    "$(BAY)"   }

{ C0I0_THR_CNT,     "Bay $(BAY) Ch 0 Int0 Threshold count",     BLM_THRCNT_$(BAY)00,    "$(BAY)"   }
{ C1I0_THR_CNT,     "Bay $(BAY) Ch 1 Int0 Threshold count",     BLM_THRCNT_$(BAY)10,    "$(BAY)"   }
{ C2I0_THR_CNT,     "Bay $(BAY) Ch 2 Int0 Threshold count",     BLM_THRCNT_$(BAY)20,    "$(BAY)"   }

{ C0I0_THR_BYTEMAP, "Bay $(BAY) Ch 0 Int 0 Threshold byte map", BLM_BYTEMAP_$(BAY)00,   "$(BAY)"   }
{ C1I0_THR_BYTEMAP, "Bay $(BAY) Ch 1 Int 0 Threshold byte map", BLM_BYTEMAP_$(BAY)10,   "$(BAY)"   }
{ C2I0_THR_BYTEMAP, "Bay $(BAY) Ch 2 Int 0 Threshold byte map", BLM_BYTEMAP_$(BAY)20,   "$(BAY)"   }

}

### Tables enable PVs ###
