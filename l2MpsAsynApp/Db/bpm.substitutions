### Threshold information ###
file "RegRO.template" { pattern
{ R,                DESC,                               PARAM,                  ADDR        }
{ X_THR_CH,         "Bay $(BAY) X Threshold channel",   BPM_THRNUM_$(BAY)0,     "$(BAY)"    }
{ Y_THR_CH,         "Bay $(BAY) Y Threshold channel",   BPM_THRNUM_$(BAY)1,     "$(BAY)"    }
{ C_THR_CH,         "Bay $(BAY) C Threshold channel",   BPM_THRNUM_$(BAY)2,     "$(BAY)"    }

{ X_THR_CNT,        "Bay $(BAY) X Threshold count",     BPM_THRCNT_$(BAY)0,     "$(BAY)"    }
{ Y_THR_CNT,        "Bay $(BAY) Y Threshold count",     BPM_THRCNT_$(BAY)1,     "$(BAY)"    }
{ C_THR_CNT,        "Bay $(BAY) C Threshold count",     BPM_THRCNT_$(BAY)2,     "$(BAY)"    }

{ X_THR_BYTEMAP,    "Bay $(BAY) X Threshold byte map",  BPM_BYTEMAP_$(BAY)0,    "$(BAY)"    }
{ Y_THR_BYTEMAP,    "Bay $(BAY) Y Threshold byte map",  BPM_BYTEMAP_$(BAY)1,    "$(BAY)"    }
{ C_THR_BYTEMAP,    "Bay $(BAY) C Threshold byte map",  BPM_BYTEMAP_$(BAY)2,    "$(BAY)"    }
}

### Tables enable PVs ###
